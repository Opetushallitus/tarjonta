<portlet-app
        xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
        version="1.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

    <portlet>
        <description></description>
        <portlet-name>TarjontaPocPortletApplication</portlet-name>
        <display-name>TarjontaPocPortletApplication</display-name>

        <portlet-class>com.vaadin.terminal.gwt.server.ApplicationPortlet2</portlet-class>

        <init-param>
            <name>application</name>
            <value>fi.vm.sade.tarjonta.poc.ui.TarjontaPocPortletApplication</value>
        </init-param>
        <init-param>
            <param-name>widgetset</param-name>
            <param-value>fi.vm.sade.widgetset.GoogleMapComponent</param-value>
        </init-param>

        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>

        <portlet-info>
            <title>TarjontaPocPortletApplication</title>
            <short-title>TarjontaPocPortletApplication</short-title>
            <keywords></keywords>
        </portlet-info>
    </portlet>

</portlet-app>
