           <xs:element name="YhteistyoMuidenToimijoidenKanssa" type="tns:MonikielinenTekstiTyyppi" minOccurs="0" maxOccurs="1"/>
            -->

            <xs:element minOccurs="0" maxOccurs="unbounded" name="tekstit" type="tns:NimettyMonikielinenTekstiTyyppi"/>

            <!--Lukiokoulutukselle spesifeja tietoja -->
            <xs:element name="LukiolinjaKoodi" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Lukiodiplomit" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="A1A2Kieli" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="B1Kieli" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="B2Kieli" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="B3Kieli" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MuutKielet" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>

            <xs:element name="viimeisinPaivittajaOid"
                        type="xs:string"
                        minOccurs="0"
                        maxOccurs="1"/>

            <xs:element name="viimeisinPaivitysPvm"
                        type="xs:dateTime"
                        minOccurs="0"
                        maxOccurs="1"/>

            <!-- optimistic locking -->
            <xs:element name="Version" type="xs:long" minOccurs="0" maxOccurs="1"/>    
            
            
            <!-- Korkeakoululle spesifeja tietoja -->     
            <xs:element name="Teemat" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="PohjakoulutusvaatimusKorkeakoulu" type="tns:KoodistoKoodiTyyppi" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Maksullisuus" type="xs:boolean" minOccurs="0" maxOccurs="1"/>           
            <xs:element name="Koulutusmoduuli" type="tns:KoulutusmoduuliKoosteTyyppi" minOccurs="1" maxOccurs="1" />         
            <xs:element name="Hinta" type="xs:string"/>
            
