            <xs:element name="Koulutustyyppi" type="tns:KoulutusasteTyyppi" minOccurs="0" maxOccurs="1"/>

            <!-- Koulutuksen lis√§tiedot (additional information) -->
            <xs:element name="Ammattinimikkeet" type="tns:KoodistoKoodiTyyppi" minOccurs="1" maxOccurs="unbounded"/>
            <!-- 
           <xs:element name="KuvailevatTiedot" type="tns:MonikielinenTekstiTyyppi" minOccurs="0" maxOccurs="1"/>
           <xs:element name="Sisalto" type="tns:MonikielinenTekstiTyyppi" minOccurs="0" maxOccurs="1"/>
           <xs:element name="SijoittuminenTyoelamaan" type="tns:MonikielinenTekstiTyyppi" minOccurs="0" maxOccurs="1"/>
