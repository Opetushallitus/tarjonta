        <!-- tutkintonimike -->
        <Qualification>
            <Title>lähihoitaja</Title>
        </Qualification>

        <!-- tutkinnon nimi -->
        <DegreeTitle>sosiaali- ja terveysalan pt</DegreeTitle>

        <Classification>

            <!-- koulutusluokituksen koodi -->
            <ClassificationCode scheme="http://stat.fi" version="2010">
                <Value>371101</Value>
                <Label xml:lang="fi">Lähihoitaja, sosiaali- ja terveysalan perustutkinto</Label>
            </ClassificationCode>

            <!-- koulutusala -->
            <ClassificationCode category="EducationDomain" scheme="http://oph.fi" version="2002">
                <Value>7</Value>
                <Label>sosiaali- terveys- ja liikunta-ala</Label>
            </ClassificationCode>

            <!-- koulutusaste -->
            <ClassificationCode category="EducationDegree" scheme="http://oph.fi" version="2002">
                <Value>32</Value>
                <Label>Ammatillinen koulutus</Label>
            </ClassificationCode>

            <!-- opintoala -->
            <ClassificationCode category="StydyDomain" scheme="http://oph.fi" version="2002">
                <Value>703</Value>
                <Label>sosiaali- ja terveysala</Label>
            </ClassificationCode>

            <!-- EQF -->
            <ClassificationCode scheme="EQF">
                <Value>4</Value>
            </ClassificationCode>

            <!-- NQF -->
            <ClassificationCode scheme="NQF">
                <Value>4</Value>
            </ClassificationCode>

        </Classification>

        <Description>

            <!-- koulutusrakenteen kuvaus -->
            <StructureDiagram>
                Jos opintotarjontaa ei ole kannassa tekstuaalinen kuvaus:
                Tutkinnon kaikille pakolliset tutkinnon osat ovat kasvun
                tukeminen ja ohjaus (15 ov), hoito ja huolenpito (20 ov)
                ja kuntoutumisen tukeminen (15 ov).  Lisäksi suoritetaan
                valitun koulutusohjelman/osaamisalan pakolliset tutkinnon
                osat. Ensihoidon koulutusohjelman/osaamisalan pakollinen
                tutkinnon osa on ensihoitopalvelussa toimiminen (20 ov)
                ja lisäksi valinnainen joko päivystyspolikliininen hoitotyö
                (10 ov) tai perioperatiivinen hoitotyö (10 ov).
            </StructureDiagram>

            <!-- jatko-opintoväylät -->
            <AccessToFurtherStudies>
                Ammatillinen perustutkinto antaa yleisen korkeakoulukelpoisuuden.
            </AccessToFurtherStudies>

        </Description>

    </LearningOpportunitySpecification>


