<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">


	<!-- Conversion service -->
    <bean id="conversionService"
              class="fi.vm.sade.generic.service.conversion.SadeConversionServiceFactoryBean">
        <property name="converters">
            <list>
                <bean class="fi.vm.sade.tarjonta.service.impl.conversion.TutkintoOhjelmaToDTOConverter"/>                
                <bean class="fi.vm.sade.tarjonta.service.impl.conversion.TutkintoOhjelmaDTOToTutkintoOhjelmaConverter"/>                
                <bean class="fi.vm.sade.tarjonta.service.impl.conversion.KoulutusmoduuliDTOToKoulutusmoduuliConverter"/>                
            </list>                
        </property>
    </bean>
	
</beans>
