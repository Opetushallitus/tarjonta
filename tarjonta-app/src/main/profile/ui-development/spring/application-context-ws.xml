<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="tarjontaService" class="fi.vm.sade.tarjonta.service.mock.TarjontaServiceMock" />
    <bean id="hakueraService" class="fi.vm.sade.tarjonta.service.mock.HakueraServiceMock" />

    <!-- Koodisto service (mock) -->
    <bean id="mockDataHandler" class="fi.vm.sade.koodisto.service.mock.MockDataHandler"/>
    
    <bean id="koodiService" class="fi.vm.sade.koodisto.service.mock.KoodiServiceMock">
        <property name="mockDataHandler" ref="mockDataHandler"/>
    </bean>
    
    <bean id="koodistoService" class="fi.vm.sade.koodisto.service.mock.KoodistoServiceMock">
        <property name="mockDataHandler" ref="mockDataHandler"/>
    </bean> 

</beans>
